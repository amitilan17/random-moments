    <!--    onboarding screens-->
    <string name="onboarding1_title">האני החווה מול האני הזוכר</string>
    <string name="onboarding1_text1">"נפריד בין שני מצבי התודעה שלנו:\nה״אני החווה״ וה״אני הזוכר״."</string>
    <string name="onboarding1_text2">חייו של “האני החווה” מורכבים ממיליוני רגעים שחולפים בזה אחר זה. הוא מכיר רק את הרגע הנוכחי, ההווה הפסיכולוגי, שנמשך כ-3 שניות.</string>
    <string name="onboarding1_text3">״האני הזוכר״ אוסף רגעים לסיפורים ומתייק אותם כזכרונות. סיפורים כאלה הם בדרך כלל בעלי מאפיינים עלילתיים: שינוי, משמעות וסיום.</string>
    <string name="onboarding2_title">לזכור הוויה, ולא רק סיפור</string>
    <string name="onboarding2_text">מי אנחנו ברגעים הסתמיים שבין רגעי השיא?\n\nמה בכל זאת קורה לנו בהם, שהרי הם רוב חיינו?\n\nמה נוכל ללמוד על עצמנו אם נתבונן ברגעים האלה, אם נתעד אותם בדיוק כפי שהם?</string>
    <string name="onboarding3_title">איך זה עובד?</string>
    <string name="onboarding3_text1">מקבלים נוטיפיקציה ברגע רנדומלי.</string>
    <string name="onboarding3_text2">כותבים את ההווה: מה אני רואה? על מה אני חושבת? מה אני מרגיש?</string>
    <string name="onboarding3_text3">מצרפים תמונה מהרגע.</string>
    <string name="onboarding3_text4">ממשיכים לאסוף רגעים במשך זמן רנדומלי שנבחר עבורך, בין 15 ל-30 יום.</string>
    <string name="onboarding3_text5">מקבלים יומן מודפס במשלוח עד הבית.</string>
    <string name="onboarding3_button">נתחיל</string>
    <string name="onboarding_bullet1">א.</string>
    <string name="onboarding_bullet2">ב.</string>
    <string name="onboarding_bullet3">ג.</string>
    <string name="onboarding_bullet4">ד.</string>
    <string name="onboarding_bullet5">ה.</string>


